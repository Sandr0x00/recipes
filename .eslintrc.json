{
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-trailing-spaces": "warn",
        "spaced-comment": "warn",
        "no-empty-function": "warn",
        "no-unused-expressions": "warn",
        "no-unreachable": "warn",
        "semi": "warn",
        "quotes": ["warn", "single"],
        "no-var": "warn",
        "no-undef": "warn",
        "no-unused-vars": "warn"
    }
}